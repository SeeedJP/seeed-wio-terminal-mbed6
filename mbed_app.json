{
    "target_overrides": {
        "WIO_TERMINAL": {
            "sd.SPI_MOSI": "SDCARD_SPI_MOSI",
            "sd.SPI_MISO": "SDCARD_SPI_MISO",
            "sd.SPI_CLK":  "SDCARD_SPI_SCK",
            "sd.SPI_CS":   "SDCARD_SPI_CS",
            "target.network-default-interface-type" : "WIFI",
            "target.components_add": ["wifi_rtl8720dn"],
            "rtl8720dn.provide-default": true,
            "nsapi.default-wifi-security": "WPA_WPA2",
            "nsapi.default-wifi-ssid": "\"YOUR SSID\"",
            "nsapi.default-wifi-password": "\"PASSWORD\"",
            "target.boot-stack-size": "0x2000",
            "platform.stdio-baud-rate": 115200,
            "target.stdio_uart_tx": "PB26",
            "target.stdio_uart_rx": "PB27",
            "target.stdio_uart": "UART_2"
        }
    }
}
